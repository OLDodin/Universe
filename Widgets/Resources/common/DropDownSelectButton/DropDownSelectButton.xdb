			<StateNormal>
				<LayerMain href="ButtonRegularNormal.(WidgetLayerSimpleTexture).xdb" />
				<FormatFileRef href="ButtonFormatNormal.txt" />
			</StateNormal>
			<ReactionOnUp>false</ReactionOnUp>
			<Reaction>SelectDropDownBtnPressed</Reaction>
		</Item>
	</Variants>
	<TextStyle>
		<Align>ALIGNY_MIDDLE</Align>
	</TextStyle>
	<Visible>false</Visible>
</WidgetButton>
